{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Current Test File",
			"autoAttachChildProcesses": true,
			"skipFiles": ["<node_internals>/**", "**/node_modules/**"],
			"program": "${relativeFile}",
			"runtimeArgs": [
				"--disable-warning=ExperimentalWarning",
				"--experimental-strip-types",
				"./scripts/test.ts",
				"test:inspect"
			],
			"smartStep": true,
			"console": "integratedTerminal"
		}
	]
}
